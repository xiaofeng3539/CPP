#include<iostream>
#include<string>
using namespace std;

//字符串比较

void test01(){
	string s1 = "aello";
	string s2 = "hello";
	if(s1.compare(s2) == 0){
		cout << "s1等于s2"  << endl;
	}else if(s1.compare(s2) > 0){
		cout << "s1大于s2" << endl;
	}else{
		cout << "s1小于s2" << endl;
	}
	
}

//  总结：字符串比较 主要是判断两个字符串是否相等，判断大小意义不大
int main(){
	test01();
	return 0;
}
